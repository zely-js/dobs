{
  "name": "dobs",
  "version": "0.0.1-alpha.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": "./dist/cli.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./_build": {
      "import": {
        "types": "./dist/exports.d.mts",
        "default": "./dist/exports.mjs"
      },
      "require": {
        "types": "./dist/exports.d.ts",
        "default": "./dist/exports.js"
      }
    }
  },
  "dependencies": {
    "@dobsjs/http": "workspace:*",
    "animaux": "^0.1.2",
    "chalk": "4",
    "chokidar": "^4.0.3",
    "deepmerge-ts": "^7.1.5",
    "module-loader-ts": "^0.0.1-alpha.0",
    "rolldown": "^1.0.0-beta.46"
  }
}
