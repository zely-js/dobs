{
  "name": "dobs",
  "version": "0.1.3",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "bin": "./dist/cli.js",
  "repository": "https://github.com/zely-js/dobs",
  "homepage": "https://dobs.vercel.app",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./_build": {
      "import": {
        "types": "./dist/builder/exports.d.mts",
        "default": "./dist/builder/exports.mjs"
      },
      "require": {
        "types": "./dist/builder/exports.d.cts",
        "default": "./dist/builder/exports.cjs"
      }
    },
    "./experimental": {
      "import": {
        "types": "./dist/experimental.d.mts",
        "default": "./dist/experimental.mjs"
      },
      "require": {
        "types": "./dist/experimental.d.cts",
        "default": "./dist/experimental.cjs"
      }
    }
  },
  "dependencies": {
    "@dobsjs/http": "workspace:*",
    "animaux": "^0.1.2",
    "chalk": "4",
    "chokidar": "^4.0.3",
    "deepmerge-ts": "^7.1.5",
    "module-loader-ts": "workspace:*",
    "rolldown": "1.0.0-beta.50",
    "source-map": "^0.7.6"
  }
}
